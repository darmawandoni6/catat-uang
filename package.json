{
  "name": "catat-uang",
  "version": "1.0.0",
  "private": "true",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "install:server": "yarn --cwd server install",
    "install:client": "yarn --cwd client install",
    "install:all": "yarn install:client && yarn install:server",
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "dev:server": "yarn --cwd server dev",
    "dev:client": "yarn --cwd client dev",
    "build:server": "yarn --cwd server build",
    "build:client": "yarn --cwd client build",
    "copy-client": "rm -rf server/client && cp -r client/dist server/client",
    "build": "yarn build:server && yarn build:client",
    "start:server": "yarn --cwd server start",
    "start": "yarn start:server"
  },
  "keywords": [
    "catat uang",
    "budgeting",
    "keuangan pribadi",
    "pemasukan",
    "pengeluaran",
    "laporan keuangan",
    "aplikasi keuangan",
    "manajemen uang",
    "uang masuk",
    "uang keluar",
    "financial tracker",
    "personal finance",
    "money management",
    "budget planner",
    "expense tracker"
  ],
  "author": {
    "name": "Doni Darmawan",
    "email": "darmawandoni6@gmail.com",
    "url": "https://darmawan.netlify.app/"
  },
  "license": "ISC",
  "description": "",
  "dependencies": {
    "concurrently": "^9.2.0"
  }
}
